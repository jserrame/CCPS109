#Revorse the vewels
def reverse_vowels(text):
    vowels = ("aeiouAEIOU")
    text_v = ""

    for i in text:
        if i in vowels:
            text_v = i + text_v


    count = 0
    ans = ''


    for i in text:
        if i in vowels:
            if i.isupper():
                ans += text_v[count].upper()
            else:
                ans += text_v[count].lower()
            count += 1
        else:
            ans += i

    return ans
